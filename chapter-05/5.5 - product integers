#include <iostream>
using namespace std;

int main() {
    int limit, num, total = 1;

    cout << "Enter number of values to be summed followed by values: ";
    cin >> limit;

    for (int i = 1; i <= limit; i++) {
        cin >> num;
        total *= num;
    }

    cout << "Total = " << total << std::endl;

    system("pause");
}
