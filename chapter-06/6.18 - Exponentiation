#include <iostream>
using namespace std;

int integerPower(int base, int exponent) {
	int result = 1;
	for (int i = 1; i <= exponent; i++) {
		result *= base;
	}
	return result;
}

int main() {
	int base, exponent;
	cout << "Enter base then exponent.";
	cin >> base >> exponent;
	cout << integerPower(base, exponent);
	system("pause");
}
